{
  "name": "NYMag News & Politics Menu Equity",
  "version": "1.0.0.1",
  "manifest_version": 2,
  "description": "Restore fairness to the nymag.com News & Politics menu",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["http://nymag.com/*"],
    "css": ["menu_equity.css"],
    "run_at": "document_start"
  }],
  "permissions": [
    "http://nymag.com/*"
  ]
}